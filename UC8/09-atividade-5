var qtd = 10

for (let cont = 0; cont <= qtd; cont++) {
    
    if (cont == 0) {
        console.log (cont + " número correspondente ZERO")
    } 
    if (cont % 2 == 0) {
            console.log (cont + " número correspondente PAR")
    } 
    if { console.log (cont + " número correspondente IMPAR")
    }
}